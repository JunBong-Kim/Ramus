<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="20dp"
    android:background="@color/background_secondary"
    android:orientation="vertical"
    android:paddingBottom="34dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingHorizontal="28dp"
        android:paddingTop="32dp"
        android:paddingBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00FFFFFF"
            android:fontFamily="@font/spoqahansans_medium"
            android:letterSpacing="-0.07"
            android:text="코로나 웹"
            android:textColor="@color/text_primary"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <HorizontalScrollView
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layout_constraintTop_toBottomOf="@id/layout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingRight="26dp"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="32dp"
                    android:id="@+id/daegu_web"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:background="@drawable/oval"
                    android:fontFamily="@font/spoqahansans_bold"
                    android:layout_marginRight="36dp"
                    android:gravity="center"
                    android:letterSpacing="-0.03"
                    android:text="DEAGU"
                    android:textColor="@color/primary"
                    android:textSize="11dp"
                    ></TextView>

                <TextView
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@id/daegu_web"
                    app:layout_constraintStart_toStartOf="@id/daegu_web"
                    app:layout_constraintEnd_toEndOf="@id/daegu_web"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqahansans_medium"
                    android:gravity="center"
                    android:letterSpacing="-0.05"
                    android:text="대구시"
                    android:textColor="@color/text_primary"
                    android:textSize="10dp"
                    ></TextView>


                <TextView
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="112dp"
                    android:id="@+id/knu_web"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:background="@drawable/oval"
                    android:fontFamily="@font/spoqahansans_bold"
                    android:layout_marginRight="36dp"
                    android:gravity="center"
                    android:letterSpacing="-0.03"
                    android:text="KNU"
                    android:textColor="@color/primary"
                    android:textSize="11dp"
                    ></TextView>



                <TextView
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@id/knu_web"
                    app:layout_constraintStart_toStartOf="@id/knu_web"
                    app:layout_constraintEnd_toEndOf="@id/knu_web"
                    android:id="@+id/daegu4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqahansans_medium"
                    android:gravity="center"
                    android:letterSpacing="-0.05"
                    android:text="KNU 코로나"
                    android:textColor="@color/text_primary"
                    android:textSize="10dp"
                    ></TextView>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="20dp"
                    android:id="@+id/naver_web"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:background="@drawable/oval"
                    android:fontFamily="@font/spoqahansans_bold"
                    android:layout_marginRight="36dp"
                    android:gravity="center"
                    android:letterSpacing="-0.03"
                    android:text="NAVER"
                    android:textColor="@color/primary"
                    android:textSize="11dp"
                    ></TextView>

                <TextView
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/naver_web"
                    app:layout_constraintStart_toStartOf="@+id/naver_web"
                    app:layout_constraintEnd_toEndOf="@+id/naver_web"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqahansans_medium"
                    android:gravity="center"
                    android:letterSpacing="-0.05"
                    android:text="네이버 실시간"
                    android:textColor="@color/text_primary"
                    android:textSize="10dp"
                    ></TextView>


                <TextView
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="100dp"
                    android:id="@+id/mohw_web"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:background="@drawable/oval"
                    android:fontFamily="@font/spoqahansans_bold"
                    android:layout_marginRight="36dp"
                    android:gravity="center"
                    android:letterSpacing="-0.03"
                    android:text="MOHW"
                    android:textColor="@color/primary"
                    android:textSize="11dp"
                    ></TextView>



                <TextView
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@id/mohw_web"
                    app:layout_constraintStart_toStartOf="@id/mohw_web"
                    app:layout_constraintEnd_toEndOf="@id/mohw_web"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqahansans_medium"
                    android:gravity="center"
                    android:letterSpacing="-0.05"
                    android:text="질병관리청"
                    android:textColor="@color/text_primary"
                    android:textSize="10dp"
                    ></TextView>

                <TextView
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="180dp"
                    android:id="@+id/gb_web"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:background="@drawable/oval"
                    android:fontFamily="@font/spoqahansans_bold"
                    android:layout_marginRight="36dp"
                    android:gravity="center"
                    android:letterSpacing="-0.03"
                    android:text="GB"
                    android:textColor="@color/primary"
                    android:textSize="11dp"
                    ></TextView>

                <TextView
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@id/gb_web"
                    app:layout_constraintStart_toStartOf="@id/gb_web"
                    app:layout_constraintEnd_toEndOf="@id/gb_web"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqahansans_medium"
                    android:gravity="center"
                    android:letterSpacing="-0.05"
                    android:text="경상북도"
                    android:textColor="@color/text_primary"
                    android:textSize="10dp"
                    ></TextView>

            </androidx.constraintlayout.widget.ConstraintLayout>


        </LinearLayout>



    </HorizontalScrollView>




</androidx.constraintlayout.widget.ConstraintLayout>